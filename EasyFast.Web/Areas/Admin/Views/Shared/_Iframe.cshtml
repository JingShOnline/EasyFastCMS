@inherits EasyFast.Web.Views.EasyFastWebViewPageBase
@{
    SetAntiForgeryCookie();
}
<html>
<head>
    <meta charset="utf-8" />
    <title>京师在线系统后台管理</title>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta content="width=device-width, initial-scale=1" name="viewport" />
    <meta name="renderer" content="webkit" />
    <link href="//cdn.jingshonline.com/scripts/common/bootstrap/3.3.7/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="//cdn.jingshonline.com/scripts/common/bootstrap-datepicker/1.6.4.1/css/bootstrap-datepicker3.min.css" rel="stylesheet">
    <link href="//cdn.jingshonline.com/scripts/common/jquery-easyui-1.5/themes/bootstrap/easyui.css" rel="stylesheet" type="text/css" />
    <link href="//cdn.jingshonline.com/metronic4.7/assets/global/plugins/bootstrap-switch/css/bootstrap-switch.min.css" rel="stylesheet" type="text/css" />
    <link href="//cdn.jingshonline.com/metronic4.7/assets/global/plugins/simple-line-icons/simple-line-icons.min.css" rel="stylesheet" type="text/css" />
    <link href="//cdn.jingshonline.com/metronic4.7/assets/global/plugins/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css" />
    <link href="//cdn.jingshonline.com/metronic4.7/assets/global/plugins/icheck/skins/all.css" rel="stylesheet" type="text/css" />
    <link href="//cdn.jingshonline.com/scripts/common/layer-2.4/skin/layer.css" rel="stylesheet" type="text/css" />
    <link href="//cdn.jingshonline.com/scripts/common/sweetalert-master/dist/sweetalert.css" rel="stylesheet" type="text/css" />
    <link href="//cdn.jingshonline.com/scripts/common/jingShOss.js/4.0/jingShOss.min.css" rel="stylesheet">
    <link href="//cdn.jingshonline.com/scripts/common/select2/css/select2.min.css" rel="stylesheet">
    <link href="~/Content/easyui.custom.min.css" rel="stylesheet" />
    <link href="~/Content/metronic.min.css" rel="stylesheet" />
    <link href="~/Content/admin.min.css" rel="stylesheet" />
    @RenderSection("css", required: false)

</head>
<body class="iframe_box">

    @RenderBody()


    @*<script src="//cdn.jingshonline.com/scripts/common/jquery/1.12.4/jquery-1.12.4.min.js" type="text/javascript"></script>*@
    <script src="//cdn.jingshonline.com/scripts/common/jquery/3.1.1/jquery-3.1.1.min.js" type="text/javascript"></script>

    <script src="//cdn.jingshonline.com/scripts/common/jquery-easyui-1.5/jquery.easyui.min.js" type="text/javascript"></script>
    <script src="//cdn.jingshonline.com/scripts/common/jquery-easyui-1.5/locale/easyui-lang-zh_CN.js" type="text/javascript"></script>

    <!--Jquery validate 验证 开始-->
    <script src="~/Scripts/jquery.validate.js"></script>
    <script src="//cdn.jingshonline.com/scripts/common/jquery-validation-1.15.1/dist/jquery.validate.min.js"></script>
    <script src="//cdn.jingshonline.com/scripts/common/jquery-validation-1.15.1/dist/localization/messages_zh.min.js"></script>
    <script src="//cdn.jingshonline.com/scripts/common/jquery.validate.unobtrusive/3.2.6/jquery.validate.unobtrusive.min.js"></script>
    <!--Jquery validate 验证 结束-->

    <script src="//cdn.jingshonline.com/scripts/common/bootstrap/3.3.7/js/bootstrap.min.js" type="text/javascript"></script>
    <script src="//cdn.jingshonline.com/metronic4.7/assets/global/plugins/bootstrap-switch/js/bootstrap-switch.min.js" type="text/javascript"></script>
    <script src="//cdn.jingshonline.com/metronic4.7/assets/global/plugins/icheck/icheck.js"></script>

    <script src="//cdn.jingshonline.com/scripts/common/bootstrap-datepicker/1.6.4.1/js/bootstrap-datepicker.min.js"></script>
    <script src="//cdn.jingshonline.com/scripts/common/bootstrap-datepicker/1.6.4.1/locales/bootstrap-datepicker.zh-CN.min.js"></script>

    <script src="//cdn.jingshonline.com/scripts/common/layer-2.4/layer.js" type="text/javascript"></script>
    <script src="//cdn.jingshonline.com/scripts/common/sweetalert-master/dist/sweetalert.min.js" type="text/javascript"></script>

    <script src="//cdn.jingshonline.com/scripts/common/webuploader/0.1.5/webuploader.js"></script>
    <script src="//cdn.jingshonline.com/scripts/common/jingShOss.js/4.0/jingShOss.min.js"></script>
    <script src="//cdn.jingshonline.com/scripts/common/select2/js/select2.min.js"></script>
    <script src="//cdn.jingshonline.com/scripts/common/jingShSelect.js/1.0/jingShSelect.js"></script>
    <script src="~/Scripts/admin.js"></script>


    @RenderSection("scripts", required: false)

    <script>
        $(".select2").on("change", function () {
            if (window.validationUnobtrusive != undefined) {
                window.validationUnobtrusive.validate();
            }
        });
    </script>
</body>
</html>
